{
  "name": "pob2-web",
  "version": "1.21.0",
  "description": "Based on original Path Of Building Community Edition",
  "productName": "Path Of Building 2 - Web",
  "author": "Dmitry Prudnikov <mail@polaz.com>",
  "type": "module",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir test/unit",
    "test:cov": "vitest run --coverage",
    "dev": "yarn proto:generate && quasar dev",
    "dev:pwa": "yarn proto:generate && quasar dev -m pwa",
    "dev:watch": "yarn proto:generate && concurrently --kill-others --names \"dev,proto\" \"quasar dev\" \"yarn proto:watch\"",
    "build": "yarn proto:generate && quasar build",
    "build:pwa": "yarn proto:generate && quasar build -m pwa",
    "dev:electron": "yarn proto:generate && quasar dev -m electron",
    "build:electron": "yarn proto:generate && quasar build -m electron -- --publish never",
    "build:electron:publish": "yarn proto:generate && quasar build -m electron",
    "type-check": "vue-tsc --noEmit",
    "postinstall": "quasar prepare",
    "proto:generate": "yarn dlx protoc --ts_out ./src/protos/ --ts_opt ts_nocheck,eslint_disable,add_pb_suffix -I ./src/proto/vendor -I ./src/proto common.proto tree.proto items.proto skills.proto build.proto leveling.proto",
    "proto:watch": "chokidar \"src/proto/**/*.proto\" -c \"yarn proto:generate\""
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.11.1",
    "@quasar/extras": "^1.17.0",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "comlink": "^4.4.2",
    "dexie": "4.3.0",
    "electron-updater": "6.7.3",
    "fflate": "0.8.2",
    "lodash-es": "^4.17.22",
    "pinia": "^3.0.4",
    "pixi.js": "^8.14.3",
    "quasar": "^2.18.6",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.7",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@electron/packager": "^19.0.1",
    "@eslint/js": "^9.39.2",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@protobuf-ts/plugin": "^2.11.1",
    "@quasar/app-vite": "^2.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "26.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.12.0",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20"
  },
  "resolutions": {
    "@electron/node-gyp": "npm:@electron/node-gyp@10.2.0-electron.2"
  }
}
