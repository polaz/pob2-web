{
  "name": "pob2-web",
  "version": "1.0.5",
  "description": "Based on original Path Of Building Community Edition",
  "productName": "Path Of Building 2 - Web",
  "author": "Dmitry Prudnikov <mail@polaz.com>",
  "type": "module",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:pwa": "quasar dev -m pwa",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "dev:electron": "quasar dev -m electron",
    "build:electron": "quasar build -m electron -- --publish never",
    "build:electron:publish": "quasar build -m electron",
    "type-check": "vue-tsc --noEmit",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "comlink": "^4.4.2",
    "dexie": "4.2.1",
    "electron-updater": "6.6.2",
    "pinia": "^3.0.4",
    "pixi.js": "^8.14.3",
    "quasar": "^2.18.6",
    "register-service-worker": "^1.7.2",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.7",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@electron/packager": "^19.0.1",
    "@eslint/js": "^9.39.2",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@quasar/app-vite": "^2.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@types/node": "^25.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.23",
    "electron": "^39.2.7",
    "electron-builder": "26.0.12",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "vite-plugin-checker": "^0.12.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20"
  },
  "resolutions": {
    "@electron/node-gyp": "npm:@electron/node-gyp@10.2.0-electron.2"
  }
}
